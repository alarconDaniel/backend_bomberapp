{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "projects": {
    "backend": {
      "type": "application",
      "root": "",
      "entryFile": "main",
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.build.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.build.json"
      }
    },
    "jwt-auth": {
      "type": "library",
      "root": "libs/jwt-auth",
      "entryFile": "index",
      "sourceRoot": "libs/jwt-auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/jwt-auth/tsconfig.build.json"
      }
    }
  },
  "compilerOptions": {
    "deleteOutDir": true
  },
  "generateOptions": {
    "spec": false
  },
  "defaultProject": "backend"
}
